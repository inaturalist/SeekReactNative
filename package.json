    "react-native-quick-actions": "^0.3.13",
    "react-native-reanimated": "^2.12.0",
    "react-native-render-html": "^5.1.0",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.4.1",
