{
  "name": "seek",
  "version": "2.17.3",
  "description": "Seek by iNaturalist repo",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "npx react-native run-android --mode release",
    "ios": "npx react-native run-ios --mode Debug",
    "ios:release": "npx react-native run-ios --device --mode Release",
    "start": "npx react-native start",
    "clean": "./scripts/clean.sh",
    "clean-start": "npx react-native clean-project-auto && npx pod-install && npm run clean && npm start -- --reset-cache",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "e2e:build:ios": "npx detox build --configuration ios.release",
    "e2e:test:ios": "npx detox test --configuration ios.release",
    "e2e:ios": "npm run e2e:build:ios && npm run e2e:test:ios",
    "add-example-model": "node scripts/add-example-model.js"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@formidable-webview/webshell": "^2.6.0",
