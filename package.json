    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.3",
    "@react-native/eslint-config": "0.78.3",
    "@react-native/metro-config": "0.78.3",
    "@react-native/typescript-config": "0.78.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-native-check-box": "^2.1.6",
    "@types/react-native-simple-radio-button": "^2.7.7",
    "@types/react-native-svg-charts": "^5.0.16",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "decompress": "^4.2.1",
    "detox": "^20.40.2",
    "eslint": "^8.57.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-testing-library": "^7.6.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "node-downloader-helper": "^2.1.9",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.2",
    "react-native-clean-project": "^4.0.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@react-native-community|@react-navigation|i18n-js)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/tests/jest.setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
